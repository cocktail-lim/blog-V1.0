<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finn.mapper.PageMapper">
    <select id="listPages" resultType="com.finn.dto.PageDTO">
        select
        id as page_id,
        page_name,
        page_description,
        page_cover
        from
        tb_page
    </select>
</mapper>
